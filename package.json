{
    "name": "my-app-frontend",
    "version": "0.0.1",
    "license": "OSL-3.0",
    "dependencies": {
        "@scandipwa/scandipwa": "4.0.0",
        "@scandipwa/scandipwa-scripts": "2.2.1",
        "express": "^4.17.1",
        "http-proxy-middleware": "^1.0.6"
    },
    "scandipwa": {
        "type": "theme",
        "parentTheme": "@scandipwa/scandipwa",
        "extensions": {}
